<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyBottomSheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:colorBackground">@android:color/white</item>
        <item name="android:height">100dp</item>
    </style>


    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorAccent">@color/lightPurpleBlue</item>
    </style>

    <style name="AppTheme.Fab" parent="Widget.Design.FloatingActionButton">
        <item name="backgroundTint">@color/lightPurpleBlue</item>
    </style>

    <style name="AppTheme.RadioButton" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:buttonTint">@color/lightPurpleBlue</item>
    </style>
</resources>